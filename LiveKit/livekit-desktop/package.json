{
  "name": "livekit-desktop",
  "version": "1.0.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "dependencies": {
    "electron-updater": "^6.3.0",
    "livekit-client": "^2.16.1"
  },
  "description": "",
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0"
  },
  "build": {
    "appId": "com.thecord.app",
    "productName": "The Cord",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "generic",
      "url": "https://github.com/Nova849/the-cord-app/releases/latest/download/"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
